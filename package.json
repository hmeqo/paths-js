{
  "name": "@hmeqo/paths",
  "version": "0.1.1",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "description": "Collection of path utilities.",
  "type": "module",
  "author": "hmeqo",
  "license": "MIT",
  "keywords": [
    "data",
    "model"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hmeqo/paths-js.git"
  },
  "bugs": {
    "url": "https://github.com/hmeqo/paths-js/issues"
  },
  "homepage": "https://github.com/hmeqo/paths-js#readme",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run lint && npm run test && npm run clean && tsup src/index.ts --dts --format esm,cjs",
    "test": "jest",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:fix": "eslint . --fix",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier src --check",
    "format": "prettier src --write"
  },
  "dependencies": {
    "dayjs": "^1.11.18",
    "decimal.js": "^10.6.0",
    "defu": "^6.1.4",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.34.0",
    "@swc/core": "^1.13.5",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.34.0",
    "jest": "^30.1.2",
    "nuxt": "^4.0.3",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  }
}
